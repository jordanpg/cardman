{
  "name": "cardman",
  "version": "1.0.0",
  "description": "electron card manager and generator",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanpg/cardman.git"
  },
  "author": "jordanpg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jordanpg/cardman/issues"
  },
  "homepage": "https://github.com/jordanpg/cardman#readme",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/jquery": "^3.5.1",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "rasterizehtml": "^1.3.0"
  },
  "main": "./src/index.js",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "cardman"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "cardman"
      }
    }
  }
}
